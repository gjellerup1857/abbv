{
  "name": "AdBlock",
  "version": "6.9.0",
  "description": "AdBlock",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/adblockinc/ext/adblock/adblock.git"
  },
  "author": "Adblock, Inc. Team",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/adblockinc/ext/adblock/adblock/issues"
  },
  "homepage": "https://gitlab.com/adblockinc/ext/adblock/adblock/-/blob/main/README.md",
  "scripts": {
    "build:dev": "npx gulp devenv",
    "build:dev:ff": "npx gulp devenv -m 2 -t firefox",
    "build:dev:chrome": "npx gulp devenv -m 3 -t chrome",
    "build:dev:edge": "npx gulp devenv -m 2 -t chrome",
    "build:dev:all": "npx gulp devenv -m 3 -t chrome && npx gulp devenv -m 2 -t firefox && npx gulp devenv -m 2 -t chrome",
    "build:release": "npx gulp build",
    "build:release:ff": "npx gulp build -m 2 -t firefox",
    "build:release:chrome": "npx gulp build -m 3 -t chrome",
    "build:release:edge": "npx gulp build -m 2 -t chrome",
    "build:release:all": "npx gulp build -m 3 -t chrome && npx gulp build -m 2 -t chrome && npx gulp build -m 2 -t firefox",
    "build:release:beta": "npx gulp build -t chrome -m 3 --manifest-path ./build/beta_manifest.base.json --basename adblockbeta",
    "build:source": "npx gulp source",
    "json-bundle": "node build/jsonBundle.cjs",
    "lint": "eslint --ext .js,.mjs adblock-betafish/ src/",
    "lint:standard": "eslint --ignore-path .gitignore src/",
    "lint:legacy": "eslint --ext .js,.mjs --ignore-path .gitignore --ignore-pattern /adblockplusui/ adblock-betafish/",
    "lint-fix": "eslint --fix --ext .js,.mjs adblock-betafish/ src/",
    "lint:standard-fix": "eslint --fix --ignore-path .gitignore src/",
    "lint:legacy-fix": "eslint --fix --ext .js,.mjs --ignore-path .gitignore --ignore-pattern /adblockplusui/ adblock-betafish/",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "html-hint": "htmlhint adblock-betafish/ -R ./rules/",
    "postinstall": "npm run build-rule-index",
    "build-rule-index": "npm explore @adblockinc/rules -- npm install && npm explore @adblockinc/rules -- npm run build adblock",
    "test:end-to-end:build": "node test/end-to-end/helper-extension/build.js",
    "test:end-to-end": "npm run test:end-to-end:build && mocha test/end-to-end/runner.js --timeout 100000 --"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-typescript": "7.18.6",
