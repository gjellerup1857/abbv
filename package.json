    "prettier-fix": "prettier --write adblock-betafish/**/*.{css,json,html}",
    "html-hint": "htmlhint adblock-betafish/ -R ./rules/",
    "postinstall": "npm run build-rule-index",
    "build-rule-index": "npm explore @adblockinc/rules -- npm run build adblock"
  },
